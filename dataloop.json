{
  "displayName": "Anonymizer",
  "name": "anonymizer",
  "description": "Takes an item, a model, and a list of labels. Calls the model predict and blurs objects with the listed labels",
  "scope": "project",
  "version": "0.1.10",
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Pipeline",
    "Pipeline Type": "Node",
    "Node Type": "Image Utils"
  },
  "components": {
    "pipelineNodes": [
      {
        "invoke": {
          "type": "function",
          "namespace": "anonymizer-service.anonymizer-module.anonymize_objects"
        },
        "name": "Anonymizer",
        "categories": ["Anonymization"],
        "displayName": "Anonymizer",
        "description": "Blur objects detected by the input model and tagged as one of the listed labels",
        "scope": "node",
        "configuration": {
          "fields": [
            {
              "name": "name",
              "title": "Node Name",
              "props": {
                "title": true,
                "type": "string",
                "default": "Anonymizer",
                "required": true,
                "placeholder": "Insert node name"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "model_id",
              "title": "Model",
              "props": {
                "title": true,
                "type": "string",
                "default": "",
                "required": true,
                "placeholder": "Insert model id"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "blur_intensity",
              "title": "Blur Intensity",
              "props": {
                "title": true,
                "type": "number",
                "default": 25,
                "max": 100,
                "min": 0,
                "step": 1,
                "required": true
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-slider"
            },
            {
              "name": "labels",
              "title": "Labels",
              "props": {
                "title": true,
                "type": "string",
                "required": true,
                "default": "person,face"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "dataset_id",
              "title": "Dataset",
              "props": {
                "title": true,
                "type": "string",
                "default": "",
                "required": false,
                "placeholder": "Insert dataset id where the output item will be saved"
              },
              "rules": [],
              "widget": "dl-input"
            },
            {
              "name": "remote_path",
              "title": "Save directory",
              "props": {
                "title": true,
                "type": "string",
                "default": "/.dataloop/blurred",
                "required": false,
                "placeholder": "Insert directory to save items"
              },
              "rules": [],
              "widget": "dl-input"
            },
            {
              "name": "prefix",
              "title": "Saved item prefix",
              "props": {
                "title": true,
                "type": "string",
                "default": "blurred",
                "required": false,
                "placeholder": "Prefix to output item filename"
              },
              "rules": [],
              "widget": "dl-input"
            },
            {
              "name": "blur",
              "title": "Blur or fill?",
              "props": {
                "title": true,
                "type": "string",
                "default": true,
                "required": false,
                "options": [
                  {
                    "value": "blur",
                    "label": "Blur"
                  },
                  {
                    "value": "fill",
                    "label": "Fill"
                  }
                ]
              },
              "rules": [],
              "widget": "dl-select"
            },
            {
              "name": "replace",
              "title": "Replace input image?",
              "props": {
                "title": true,
                "type": "string",
                "default": true,
                "required": false,
                "options": [
                  {
                    "value": "yes",
                    "label": "Yes"
                  },
                  {
                    "value": "no",
                    "label": "No"
                  }
                ]
              },
              "rules": [],
              "widget": "dl-select"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "anonymizer-module",
        "entryPoint": "anonymizer.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "anonymize_objects",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "blurred_item",
                "actions": [
                  "anonymized",
                  "no-objects"
                ]
              }
            ],
            "displayIcon": "icon-dl-governance-filled"
          }
        ]
      }
    ],
    "services": [
      {
        "name": "anonymizer-service",
        "moduleName": "anonymizer-module",
        "initParams": {},
        "secrets": [],
        "runtime": {
          "podType": "regular-l",
          "runnerImage": "dataloop_runner-cpu/main:1.89.29.latest",
          "numReplicas": 1,
          "concurrency": 1,
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 10
          },
          "preemptible": false
        },
        "operation": "none"
      }
    ]
  }
}